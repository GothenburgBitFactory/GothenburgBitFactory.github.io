ssh twa
sudo su -
cd /var/log/apache2
tar czvf /tmp/tw.tar.gz taskwarrior.org-{access,error}.log{,.1,.?.gz}
tar czvf /tmp/tt.tar.gz tasktools.org-{access,error}.log{,.1,.?.gz}

scp twa:/tmp/t?.tar.gz .
tar xzvf tw.tar.gz
tar xzvf tt.tar.gz
gunzip *gz

ssh twa
sudo su -
rm /tmp/t?.tar.gz

cat taskwarrior.org-access* > twa.log
cat taskwarrior.org-error*  > twe.log
cat tasktools.org-access*   > tta.log
cat tasktools.org-error*    > tte.log
rm taskwarrior.org* tasktools.org* t?.tar

./downloads [Mar [2015]] twa.log tta.log
./latest    [Mar [2015]] tta.log
./404s      [Mar [2015]] twe.log tte.log
./popular   [Mar [2015]] twa.log tta.log

