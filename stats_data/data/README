ssh twa
sudo su -
cd /var/log/apache2
tar czvf /tmp/tw.tar.gz taskwarrior.org*
tar czvf /tmp/tt.tar.gz tasktools.org*
^D
^D

scp twa:/tmp/t?.log.gz .
tar xzvf tw.tar.gz
tar xzvf tt.tar.gz
gunzip *gz

ssh twa
sudo su -
rm /tmp/t?.log.gz
^D
^D

cat taskwarrior.org-access* > twa.log
cat taskwarrior.org-error*  > twe.log
cat tasktools.org-access*   > tta.log
cat tasktools.org-error*    > tte.log
rm taskwarrior.org* tasktools.org*

./downloads Mar twa.log
./latest    Mar tta.log
./404s      Mar twe.log
./404s      Mar tte.log
./popular   Mar twa.log
./popular   Mar tta.log

